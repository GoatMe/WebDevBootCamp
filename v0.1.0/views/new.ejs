<%- include ("partials/header")%>
<div class="container">
		<h1>Create a New Campground</h1>
	<div class="row">
		<form action="/campgrounds" method="POST">
			<div class="form-group col-auto">
				<input class="form-control" type="text" name= "name" placeholder="name">
			</div>
			<div class="form-group col-auto">
				<input class="form-control" type="text" name= "img" placeholder="image url">
			</div>
			<div class="form-group col-auto">
				<input class="form-control" type="text" name= "desc" placeholder="desc">
			</div>
			<div class="form-group col-auto">
				<button class="btn btn-md btn-outline-primary btn-block">Submit</button>
			</div>
		</form>

	</div>
		<a href="/campgrounds">Go Back</a>
</div>

<%- include ("partials/footer")%>